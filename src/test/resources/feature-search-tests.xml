<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Search Feature Suite" parallel="methods" thread-count="4" verbose="1">
    
    <listeners>
        <listener class-name="com.amazon.automation.tests.listeners.ExtentTestListener"/>
    </listeners>
    
    <test name="Search Tests">
        <groups>
            <run>
                <include name="search"/>
            </run>
        </groups>
        <packages>
            <package name="com.amazon.automation.tests.search"/>
        </packages>
    </test>
    
</suite>