<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cart Feature Suite" parallel="classes" thread-count="3" verbose="1">
    
    <listeners>
        <listener class-name="com.amazon.automation.tests.listeners.ExtentTestListener"/>
    </listeners>
    
    <test name="Cart Tests">
        <groups>
            <run>
                <include name="cart"/>
            </run>
        </groups>
        <packages>
            <package name="com.amazon.automation.tests.cart"/>
        </packages>
    </test>
    
</suite>